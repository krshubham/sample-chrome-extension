{
    "name": "Sample Chrome Extension",
    "version": "1.0",
    "description": "Sample Chrome extension to learn more about it.",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["storage"],
    "page_action": {
        "default_popup": "popup.html"
    }
}